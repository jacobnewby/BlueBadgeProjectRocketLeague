@model IEnumerable<RocketLeague.Models.WheelsListItem>

@{
    ViewBag.Title = "Index";
}

<h2>Wheels</h2>

<p>
    <a href="~/Wheels/Create" class="custombtn">Create</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WheelsID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WheelsName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WheelsColor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WheelsRarity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.WheelsID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WheelsName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WheelsColor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WheelsRarity)
        </td>
        <td>
            <a href="~/Wheels/Edit/@item.WheelsID" class="custombtn">Edit</a> |
            <a href="~/Wheels/Delete/@item.WheelsID" class="custombtn">Delete</a>
        </td>
    </tr>
}

</table>
